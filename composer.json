{
  "name": "bynder/webdam-php-sdk",
  "description": "SDK in PHP for integrations with WebDAM",
  "license": "MIT",
  "homepage": "https://github.com/Bynder/webdam-php-sdk",
  "require": {
    "php": ">=5.6.0",
    "guzzlehttp/guzzle": "^5 || ^6 || ^7",
    "guzzlehttp/psr7": "^1 || ^2"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Bynder\\webdam\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bynder\\webdam\\tests\\": "tests"
    }
  }
}
